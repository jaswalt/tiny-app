<% include ./partials/_header %>

<body>
    <h2>URLs</h2>
      <ul>
        <% for(url in urls) { %>
          <li>
            <%= url %> : <%= urls[url].longURL %>
          </li>
          <form method="POST" action="/urls/<%= url %>/delete">
            <input type="submit" value="Delete">
          </form>
        <% }; %>
      </ul>

      <p><a href=>Shorten New Link</a></p>

</body>